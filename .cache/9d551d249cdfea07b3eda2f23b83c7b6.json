oup-v4/.test(g)) {\n      return g;\n    }\n\n    return sprintf('<span class=\"hover-group group-%d\">%s</span>',\n      i + offset,\n      spanLeadingZeroesSimple(g));\n  }).join(':');\n};\n"},"lineCount":null}},"hash":"c9a1278d8c8871352ff7bcf7cd65fe3a","cacheData":{"env":{}}}